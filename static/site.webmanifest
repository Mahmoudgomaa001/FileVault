{
  "name": "FileVault",
  "short_name": "FileVault",
  "scope": "/",
  "start_url": "/static/launcher.html",
  "display": "standalone",
  "background_color": "#ffe6f2",
  "theme_color": "#ff4fa3",
  "icons": [
    {
      "src": "/static/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml"
    }
  ],
  "share_target": {
    "action": "/share.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "files",
          "accept": ["*/*"]
        }
      ]
    }
  }
}